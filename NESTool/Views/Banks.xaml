            <StackPanel Grid.Column="2" Grid.Row="1"
                        VerticalAlignment="Center">
                <Button Height="32" Width="32"
                        Command="{Binding MoveTileToBankCommand}">
                    <Button.CommandParameter>
                        <MultiBinding Converter="{StaticResource parametersConverter}">
                            <Binding Path="CroppedImage" />
                            <Binding Path="CroppedPoint" />
                            <Binding Path="SelectedTileSet" />
                            <Binding Path="Model" />
                        </MultiBinding>
                    </Button.CommandParameter>
                    <Image Width="32" Height="32" 
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Source="/resources/left_arrow.png">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Style.Triggers>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter Property="Effect">
                                            <Setter.Value>
                                                <fx:GrayscaleEffect />
                                            </Setter.Value>
                                        </Setter>
                                        <Setter Property="Opacity" Value="0.5"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </Image>
                </Button>
