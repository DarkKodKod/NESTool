<UserControl x:Class="NESTool.Views.Banks"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:local="clr-namespace:NESTool.Views"
             xmlns:ex="clr-namespace:NESTool.Utils.Extensions"
             xmlns:enum="clr-namespace:NESTool.Enums"
             xmlns:behaviors="clr-namespace:NESTool.Utils.Behaviors"
             xmlns:fx="clr-namespace:ArchitectureLibrary.FXs;assembly=ArchitectureLibrary"
             xmlns:cv="clr-namespace:NESTool.Utils.Converters"
             xmlns:vm="clr-namespace:NESTool.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="690" d:DesignWidth="850">
    <UserControl.Resources>
        <cv:ParametersConverter x:Key="parametersConverter"/>
    </UserControl.Resources>
    <UserControl.DataContext>
        <vm:BanksViewModel />
    </UserControl.DataContext>
    <Grid Background="LightGray">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="10"/>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="40"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="10"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="10"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="10"/>
            </Grid.RowDefinitions>
            <Grid Grid.Column="1" Grid.Row="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="3"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="3"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Horizontal" Grid.Row="0">
                    <Border BorderThickness="0.8" BorderBrush="Gray">
                        <TextBlock Width="80" Background="White"
                                   Foreground="Gray"
                                   VerticalAlignment="Stretch"
                                   TextAlignment="Center"
                                   Text="{Binding ProjectGridSize}">
                        </TextBlock>
                    </Border>
                    <ComboBox Margin="10,0,0,0" Width="200"
                              ItemsSource="{Binding Source={ex:EnumBindingSource {x:Type enum:PatternTableType}}}"
                              SelectedItem="{Binding Path=SelectedPatternTableType}"/>
                    <RadioButton GroupName="Grouped" Margin="10,0,0,0" IsChecked="True">Compact</RadioButton>
                    <RadioButton GroupName="Grouped" Margin="10,0,0,0" Focusable="False" IsHitTestVisible="False">Free form</RadioButton>
                </StackPanel>
                <StackPanel Orientation="Horizontal" Grid.Row="2">
                    <Button Width="100" Command="{Binding DeleteBankTileCommand}"
                            Content="Delete Cell">
                        <Button.CommandParameter>
                            <MultiBinding Converter="{StaticResource parametersConverter}">
                                <Binding Path="SelectedPatternTableTile" />
                                <Binding Path="Model" />
                            </MultiBinding>
                        </Button.CommandParameter>
                    </Button>
                    <TextBlock Margin="10,0,0,0" VerticalAlignment="Center">Group:</TextBlock>
                    <TextBox Width="30"
                             Margin="3,0,0,0"
                             VerticalAlignment="Stretch"
                             VerticalContentAlignment="Center"
                             Text="{Binding SelectedGroup, UpdateSourceTrigger=PropertyChanged}">
                    </TextBox>
                    <Label Margin="10,0,0,0">Hex Index:</Label>
                    <TextBlock VerticalAlignment="Center" Text="{Binding SelectedIndex}" />
                </StackPanel>
                <Border Grid.Row="4" Width="510" Height="510"
                        VerticalAlignment="Top">
                    <Canvas Width="128" Height="128" 
		                Background="DarkGray"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top">
                        <Canvas.RenderTransform>
                            <ScaleTransform ScaleX="4" ScaleY="4"/>
                        </Canvas.RenderTransform>
                        <Image RenderOptions.BitmapScalingMode="NearestNeighbor"
                               Source="{Binding PTImage}"
	                           Width="128"
	                           Height="128"
                               Name="imgPatternTable"
	                           Stretch="UniformToFill">
                            <i:Interaction.Behaviors>
                                <behaviors:EventToCommandBehavior Command="{Binding ImageMouseDownCommand}" Event="MouseDown" PassArguments="True" />
                            </i:Interaction.Behaviors>
                        </Image>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" /><RowDefinition Height="8" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" /><ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding CellGroup[0]}" Background="White" Foreground="Black" FontSize="3" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="{Binding GroupMarkVisible}" />
